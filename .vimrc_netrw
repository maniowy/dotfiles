" :Explore options (native vim filesystem tree)
let g:netrw_liststyle=3 " tree style file listing
let g:netrw_altv=1 " right vertical split in netrw
"let g:netrw_alto=1 " bottom horizontal split in netrw
let g:netrw_browse_split=2 " 1 opens in horizontal split, 2 opens in vertical split, 3 opens in a new tab, 4 opens in previous window
let g:netrw_winsize=16 " 16% window size when making :Lexplore
""let g:netrw_keepdir=0
